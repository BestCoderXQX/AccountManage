<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="acffo.xqx.accountmanageacffo">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />

    <application
        android:name="acffo.xqx.accountmanageacffo.XApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="acffo.xqx.accountmanageacffo.ui.main.EnterMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="acffo.xqx.accountmanageacffo.ui.addAccount.AddAccountActivity" />
        <activity android:name="acffo.xqx.accountmanageacffo.ui.addAccount.SelectAppActivity" />
        <activity android:name="acffo.xqx.accountmanageacffo.ui.main.SetAdminPsActivity" />
        <activity android:name="acffo.xqx.accountmanageacffo.MainActivity" />
        <activity android:name="acffo.xqx.accountmanageacffo.ui.main.SettingActivity" />
        <activity android:name="acffo.xqx.accountmanageacffo.ui.main.DeepGuardActivity" />
        <activity android:name="acffo.xqx.accountmanageacffo.ui.addFromAccount.AddFromAccountActivity" />
        <activity android:name="acffo.xqx.accountmanageacffo.ui.main.SetDeepGuardPSActivity" />
        <activity android:name="acffo.xqx.accountmanageacffo.ui.main.AboutActivity"></activity>
    </application>

</manifest>